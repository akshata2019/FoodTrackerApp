<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
   >
    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/toolbar2">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:text="Track Your Food"
                android:textSize="20dp"
                android:fontFamily="sans-serif"
                android:textColor="#000000"/>

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignBottom="@+id/textView6"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="13dp"
                android:src="@drawable/user"
                android:id="@+id/imageView" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentStart="true"
                android:layout_alignTop="@+id/imageView"
                android:src="@drawable/ic_perm_contact_calendar_black_24dp" />


        </RelativeLayout>


    </android.support.v7.widget.Toolbar>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/toolbar2"
        android:background="#FDFBFB"
        android:id="@+id/scrollView2">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/mainLaout">

            <RelativeLayout
                android:id="@+id/relativelayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="70dp"
                android:layout_marginRight="70dp"
                android:layout_marginTop="50dp"
                android:background="@drawable/roundedrelativelayout">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_margin="5dp"
                    android:fontFamily="sans-serif"
                    android:text="0000 Calories left"
                    android:textColor="#000000"
                    android:textSize="15dp" />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayoutTwo"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_below="@id/relativelayout"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="70dp"
                android:layout_marginRight="70dp"
                android:background="@drawable/roundedrelativelayoutbottom">

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="38dp"
                    android:layout_marginTop="18dp"
                    android:fontFamily="sans-serif"
                    android:text="0"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginBottom="14dp"
                    android:layout_marginStart="12dp"
                    android:fontFamily="sans-serif"
                    android:text="Consumed"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/textView7"
                    android:layout_centerHorizontal="true"
                    android:fontFamily="sans-serif"
                    android:text="0"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView8"
                    android:layout_alignBottom="@+id/textView8"
                    android:layout_centerHorizontal="true"
                    android:fontFamily="sans-serif"
                    android:text="Burned"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView9"
                    android:layout_alignBottom="@+id/textView9"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="27dp"
                    android:fontFamily="sans-serif"
                    android:text="0"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView10"
                    android:layout_alignBottom="@+id/textView10"
                    android:layout_alignEnd="@+id/textView11"
                    android:fontFamily="sans-serif"
                    android:text="Net"
                    android:textColor="#000000" />
            </RelativeLayout>

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_alignBottom="@+id/textView16"
                android:layout_alignStart="@+id/relativelayout"
                android:src="@drawable/back" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_alignBottom="@+id/textView16"
                android:layout_alignEnd="@+id/relativelayout"
                android:src="@drawable/next" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="19dp"
                android:fontFamily="sans-serif"
                android:text="Today"
                android:textColor="#000000"
                android:textSize="15dp" />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:layout_below="@id/relativeLayoutTwo"
                android:layout_marginTop="30dp"
                >

                <ImageView
                    android:id="@+id/brackfast_image"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/roundedcorner"
                    android:src="@drawable/coffee"
                    android:layout_marginLeft="70dp"

                    />

                <ImageView
                    android:id="@+id/lunch_image"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="170dp"
                    android:background="@drawable/roundedcorner"
                    android:src="@drawable/fastfood" />

                <ImageView
                    android:id="@+id/dinner_image"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="270dp"
                    android:background="@drawable/roundedcorner"
                    android:src="@drawable/pizza" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/brackfast_image"
                    android:text="BreakFast"
                    android:layout_marginLeft="60dp"
                    android:textColor="#000000"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/lunch_image"
                    android:layout_marginLeft="175dp"
                    android:text="Lunch"
                    android:textColor="#000000"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/dinner_image"
                    android:layout_marginLeft="275dp"
                    android:text="Dinner"
                    android:textColor="#000000"
                    android:textSize="15dp" />

                <ImageView
                    android:id="@+id/exersise"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_below="@+id/textView18"
                    android:layout_marginLeft="170dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/roundedcorner"
                    android:src="@drawable/warrior" />
                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/exersise"
                    android:layout_marginLeft="170dp"
                    android:text="Exercise"
                    android:textColor="#000000"
                    android:textSize="15dp" />
            </RelativeLayout>





        </RelativeLayout>




    </ScrollView>


</RelativeLayout>
